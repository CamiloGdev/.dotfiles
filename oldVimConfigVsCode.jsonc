{
	/* vim */
	// "vim.foldfix": true,
	"vim.surround": true,
	"vim.smartRelativeLine": true,
	"vim.leader": "<Space>",

	"vim.normalModeKeyBindingsNonRecursive": [
		// FILE TREE
		{
			"before": ["<leader>", "e"],
			"commands": ["workbench.explorer.fileView.focus"]
		},
		{
			"before": ["<leader>", "b"],
			"commands": ["workbench.action.toggleSidebarVisibility"]
		}
	]
}
